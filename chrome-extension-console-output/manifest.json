{
    "manifest_version": 3,
    "name": "ConsoleTextSearch",
    "version": "1.0",
    "description": "Search for a text on the page",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  